<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unicam.cs.ids.c3.model.Vista.commerciante.gestisciPromozioniController">
   <children>
      <ListView fx:id="promozioniList" layoutX="100.0" layoutY="93.0" prefHeight="200.0" prefWidth="200.0" />
      <ComboBox fx:id="negozioComboBox" layoutX="50.0" layoutY="39.0" prefWidth="150.0" promptText="scegli un negozio..." />
      <Button fx:id="cercaButton" layoutX="436.0" layoutY="45.0" mnemonicParsing="false" onAction="#cercaPressed" text="cerca" />
      <Button fx:id="aggiungiButton" layoutX="431.0" layoutY="123.0" mnemonicParsing="false" onAction="#aggiungiPressed" text="aggiungi" />
      <Button fx:id="scontoButton" layoutX="355.0" layoutY="185.0" mnemonicParsing="false" onAction="#scontoPressed" text="modifica sconto" />
      <Button fx:id="attivaDisattivaButton" layoutX="376.0" layoutY="254.0" mnemonicParsing="false" onAction="#attivaDisattivaPressed" text="attiva/disattiva promozione" />
      <Button fx:id="rimuoviButton" layoutX="435.0" layoutY="307.0" mnemonicParsing="false" onAction="#rimuoviPressed" text="rimuovi" />
      <TextField fx:id="nuovoScontoLabel" layoutX="483.0" layoutY="187.0" prefHeight="26.0" prefWidth="110.0" promptText="nuovo sconto..." />
   </children>
</AnchorPane>
